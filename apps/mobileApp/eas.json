{
  "cli": {
    "version": ">= 18.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://leniently-unlimited-sheatfish.cloudpub.ru"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://leniently-unlimited-sheatfish.cloudpub.ru"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://leniently-unlimited-sheatfish.cloudpub.ru"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
