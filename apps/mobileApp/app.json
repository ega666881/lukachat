{
  "expo": {
    "name": "lukaChat",
    "slug": "lukaChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lukachatLogo.png",
    "scheme": "mobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1E1E1E",
        "foregroundImage": "./assets/images/lukachatLogo.png",
        "backgroundImage": "./assets/images/lukachatLogo.png",
        "monochromeImage": "./assets/images/lukachatLogo.png"
      },
      "androidManifest": {
        "application": {
          "usesCleartextTraffic": true
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.luka.lukaChat"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lukachatLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0A0A",
          "dark": {
            "backgroundColor": "#0A0A0A"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "815db81b-6653-41ca-b172-fb6b5346fbcd"
      }
    }
  }
}
